{
   "printWidth": 120,
   "tabWidth": 3,
   "useTabs": false,
   "semi": true,
   "singleQuote": true,
   "trailingComma": "es5",
   "bracketSpacing": true,
   "jsxBracketSameLine": false,
   "arrowParens": "always",
   "endOfLine": "lf",
   "plugins": ["prettier-plugin-organize-imports"],
   "overrides": [
      {
         "files": ["*.md"],
         "options": {
            "printWidth": 120
         }
      },
      {
         "files": ["*.yml", "*.yaml"],
         "options": {
            "singleQuote": false
         }
      }
   ]
}
